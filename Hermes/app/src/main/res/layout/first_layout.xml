<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/transparent_1"
        android:layout_centerHorizontal="true"
        android:id="@+id/imageView2"
        android:layout_below="@+id/textView3" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/transparent_2"
        android:layout_centerHorizontal="true"
        android:id="@+id/imageView3"
        android:layout_below="@+id/textView3" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/transparent_3"
        android:layout_centerHorizontal="true"
        android:id="@+id/imageView4"
        android:layout_below="@+id/textView3" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/transparent_4"
        android:layout_centerHorizontal="true"
        android:id="@+id/imageView5"
        android:layout_below="@+id/textView3" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/transparent_5"
        android:layout_centerHorizontal="true"
        android:id="@+id/imageView6"
        android:layout_below="@+id/textView3" />

    <TextView
        android:text="1) Pressure Distribution"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        tools:text="1) Pressure Distribution"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="17dp" />
</RelativeLayout>